{
  "name": "desktop-app",
  "version": "1.0.0-alpha",
  "author": "Eyas",
  "description": "desktop app startkit",
  "license": "BSD",
  "main": "./dist/electron/main.js",
  "_moduleAliases": {
    "@root": ".",
    "@": "src/main/app",
    "@app": "src/main/app",
    "@libs": "src/main/libs",
    "@main": "src/main",
    "@config": "src/main/config"
  },
  "scripts": {
    "build": "npm run build:win",
    "webpack:main": "node scripts/build.js --main",
    "build:dir": "node scripts/build.js && electron-builder build --config scripts/builder.json --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node scripts/build.js",
    "build:arm64": "node scripts/build.js && electron-builder build --config scripts/builder.json --arm64",
    "build:linux": "node scripts/build.js && electron-builder build --config scripts/builder.json --linux --x64",
    "build:linux32": "node scripts/build.js && electron-builder build --config scripts/builder.json --linux --ia32",
    "build:mac": "node scripts/build.js && electron-builder build --config scripts/builder.json --mac --x64",
    "build:win": "node scripts/build.js && electron-builder build --config scripts/builder.json --win --x64",
    "build:winx32": "node scripts/build.js && electron-builder build --config scripts/builder.json --win --ia32",
    "dev": "cross-env NODE_ENV=development node scripts/dev-runner.js",
    "dev:main": "cross-env NODE_ENV=development electron src/main/index.dev.js",
    "dev:renderer": "cross-env NODE_ENV=development node scripts/dev-runner.js --renderer",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "node scripts/build.js --main",
    "pack:renderer": "node scripts/build.js --renderer",
    "postinstall": "install-app-deps"
  },
  "dependencies": {
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.3",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^12.1.1",
    "electron-updater": "^3.1.2",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "import-modules": "^1.1.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "module-alias": "^2.1.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "ramda": "^0.25.0",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.4.1",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.4.2",
    "vuex": "^3.0.1",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.1.7",
    "webpack-hot-middleware": "^2.18.2",
    "winston": "^3.0.0"
  }
}
